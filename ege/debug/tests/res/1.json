{
	"classes": {
		"ip": {
			"fields": [
				{
					"type": "Array", 
					"count": "4"
				}
			], 
			"draw": "str_pattern", 
			"drawPattern": "%d.%d.%d.%d", 
			"parsePattern": "(\\d+).(\\d+).(\\d+).(\\d+)",
			"edit": "text"
		},
		"ip+mask": {
			"fields": [
				{
					"type": "ip",
					"name": "ip"
				},
				{
					"type": "Integer",
					"name": "mask"
				}
			], 
			"draw": "str_pattern", 
			"drawPattern": "%s/%d", 
			"parsePattern": ".*\/\\d*", 
			"edit": "text"
		},
		"iface_port": {
			"fields": [
				{
					"type": "Integer",
					"name": "iface_port"
				},
				{
					"type": "Integer",
					"name": "iface_id"
				}
			],
			"draw": "str_pattern",
			"drawPattern": "%d at %d",
			"edit": "event",
			"editEvent": ""
		},
		"iptable": {
			"fields": [
				{
					"type": "ip+mask",
					"name": "address"
				},
				{
					"type": "ip",
					"name": "next_router"
				},
				{
					"type": "iface_port",
					"name": "interface_port"
				}
			], 
			"draw": "table", 
			"edit": "dialog"
		}
	},
